{
    "version": 2, 
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        { "src": "*.html",
          "use": "@vercel/static" 
        },
        {
           "src": "*.json", 
           "use": "@vercel/node" 
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}